#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/ant/Projects/svelte-validation/node_modules/.pnpm/mocha@9.1.3/node_modules/mocha/bin/node_modules:/home/ant/Projects/svelte-validation/node_modules/.pnpm/mocha@9.1.3/node_modules/mocha/node_modules:/home/ant/Projects/svelte-validation/node_modules/.pnpm/mocha@9.1.3/node_modules:/home/ant/Projects/svelte-validation/node_modules/.pnpm/node_modules:/home/ant/Projects/svelte-validation/node_modules:/home/ant/Projects/node_modules:/home/ant/node_modules:/home/node_modules:/node_modules:/home/ant/Projects/svelte-validation/node_modules/mocha/bin/node_modules:/home/ant/Projects/svelte-validation/node_modules/mocha/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/ant/Projects/svelte-validation/node_modules/.pnpm/mocha@9.1.3/node_modules/mocha/bin/node_modules:/home/ant/Projects/svelte-validation/node_modules/.pnpm/mocha@9.1.3/node_modules/mocha/node_modules:/home/ant/Projects/svelte-validation/node_modules/.pnpm/mocha@9.1.3/node_modules:/home/ant/Projects/svelte-validation/node_modules/.pnpm/node_modules:/home/ant/Projects/svelte-validation/node_modules:/home/ant/Projects/node_modules:/home/ant/node_modules:/home/node_modules:/node_modules:/home/ant/Projects/svelte-validation/node_modules/mocha/bin/node_modules:/home/ant/Projects/svelte-validation/node_modules/mocha/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../mocha/bin/_mocha" "$@"
else
  exec node  "$basedir/../mocha/bin/_mocha" "$@"
fi
