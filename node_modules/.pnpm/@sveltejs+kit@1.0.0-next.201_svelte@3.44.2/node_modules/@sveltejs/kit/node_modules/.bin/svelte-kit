#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/ant/Projects/svelte-validation/node_modules/.pnpm/@sveltejs+kit@1.0.0-next.201_svelte@3.44.2/node_modules/@sveltejs/kit/node_modules:/home/ant/Projects/svelte-validation/node_modules/.pnpm/@sveltejs+kit@1.0.0-next.201_svelte@3.44.2/node_modules/@sveltejs/node_modules:/home/ant/Projects/svelte-validation/node_modules/.pnpm/@sveltejs+kit@1.0.0-next.201_svelte@3.44.2/node_modules:/home/ant/Projects/svelte-validation/node_modules/.pnpm/node_modules:/home/ant/Projects/svelte-validation/node_modules:/home/ant/Projects/node_modules:/home/ant/node_modules:/home/node_modules:/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/ant/Projects/svelte-validation/node_modules/.pnpm/@sveltejs+kit@1.0.0-next.201_svelte@3.44.2/node_modules/@sveltejs/kit/node_modules:/home/ant/Projects/svelte-validation/node_modules/.pnpm/@sveltejs+kit@1.0.0-next.201_svelte@3.44.2/node_modules/@sveltejs/node_modules:/home/ant/Projects/svelte-validation/node_modules/.pnpm/@sveltejs+kit@1.0.0-next.201_svelte@3.44.2/node_modules:/home/ant/Projects/svelte-validation/node_modules/.pnpm/node_modules:/home/ant/Projects/svelte-validation/node_modules:/home/ant/Projects/node_modules:/home/ant/node_modules:/home/node_modules:/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../svelte-kit.js" "$@"
else
  exec node  "$basedir/../../svelte-kit.js" "$@"
fi
