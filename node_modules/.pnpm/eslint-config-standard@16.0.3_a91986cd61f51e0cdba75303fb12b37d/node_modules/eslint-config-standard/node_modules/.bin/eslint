#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/ant/Projects/svelte-validation/node_modules/.pnpm/eslint@8.3.0/node_modules/eslint/bin/node_modules:/home/ant/Projects/svelte-validation/node_modules/.pnpm/eslint@8.3.0/node_modules/eslint/node_modules:/home/ant/Projects/svelte-validation/node_modules/.pnpm/eslint@8.3.0/node_modules:/home/ant/Projects/svelte-validation/node_modules/.pnpm/node_modules:/home/ant/Projects/svelte-validation/node_modules:/home/ant/Projects/node_modules:/home/ant/node_modules:/home/node_modules:/node_modules:/home/ant/Projects/svelte-validation/node_modules/.pnpm/eslint-config-standard@16.0.3_a91986cd61f51e0cdba75303fb12b37d/node_modules/eslint-config-standard/node_modules:/home/ant/Projects/svelte-validation/node_modules/.pnpm/eslint-config-standard@16.0.3_a91986cd61f51e0cdba75303fb12b37d/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/ant/Projects/svelte-validation/node_modules/.pnpm/eslint@8.3.0/node_modules/eslint/bin/node_modules:/home/ant/Projects/svelte-validation/node_modules/.pnpm/eslint@8.3.0/node_modules/eslint/node_modules:/home/ant/Projects/svelte-validation/node_modules/.pnpm/eslint@8.3.0/node_modules:/home/ant/Projects/svelte-validation/node_modules/.pnpm/node_modules:/home/ant/Projects/svelte-validation/node_modules:/home/ant/Projects/node_modules:/home/ant/node_modules:/home/node_modules:/node_modules:/home/ant/Projects/svelte-validation/node_modules/.pnpm/eslint-config-standard@16.0.3_a91986cd61f51e0cdba75303fb12b37d/node_modules/eslint-config-standard/node_modules:/home/ant/Projects/svelte-validation/node_modules/.pnpm/eslint-config-standard@16.0.3_a91986cd61f51e0cdba75303fb12b37d/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../eslint@8.3.0/node_modules/eslint/bin/eslint.js" "$@"
else
  exec node  "$basedir/../../../../../eslint@8.3.0/node_modules/eslint/bin/eslint.js" "$@"
fi
